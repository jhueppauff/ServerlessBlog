{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "IndexPage": {
            "matchCondition": {
                "route": "/",
                "methods": ["GET"]
            },
            "backendUri": "http://%WEBSITE_HOSTNAME%/api/static/GetIndexPage/"
        },
        "AddPage": {
            "matchCondition": {
                "route": "/Add",
                "methods": ["GET"]
            },
            "backendUri": "http://%WEBSITE_HOSTNAME%/api/static/GetAddPostPage"
        },
        "EditPage": {
            "matchCondition": {
                "route": "/Edit/{slug}",
                "methods": ["GET"]
            },
            "backendUri": "http://%WEBSITE_HOSTNAME%/api/static/GetEditPostPage/{slug}"
        }
    }
}